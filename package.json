{
  "name": "instant-meeting",
  "private": true,
  "version": "0.0.0",
  "homepage": "https://meet.asea.fun",
  "author": "Asea",
  "description": "Instant Meeting",
  "main": "dist-electron/main.js",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "serve": "nodemon ./backend/server.js",
    "lint": "eslint **/*.jsx",
    "electron:serve": "electron .",
    "electron:build": "electron-builder",
    "electron:rebuild": "npx electron-rebuild"
  },
  "build": {
    "appId": "fun.asea.meet",
    "asar": false,
    "icon": "public/favicon.ico",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist-electron",
      "dist"
    ],
    "mac": {
      "artifactName": "${productName}_${version}.${ext}",
      "target": [
        "dmg"
      ]
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ],
      "artifactName": "${productName}_${version}.${ext}"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": false,
      "allowToChangeInstallationDirectory": true,
      "deleteAppDataOnUninstall": false
    },
    "publish": [
      {
        "provider": "github",
        "owner": "AAAAsea",
        "repo": "instant-meeting",
        "private": true,
        "token": "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",
        "releaseType": "release"
      }
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test"
    }
  },
  "dependencies": {
    "electron-updater": "^5.3.0",
    "robotjs": "^0.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@mui/icons-material": "^5.11.11",
    "@mui/lab": "5.0.0-alpha.122",
    "@mui/material": "^5.11.12",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^2.2.0",
    "animate.css": "^4.1.1",
    "color": "^4.2.3",
    "electron": "^23.1.3",
    "electron-builder": "^23.6.0",
    "electron-is-dev": "^2.0.0",
    "electron-packager": "^17.1.1",
    "electron-rebuild": "^3.2.9",
    "esbuild": "^0.17.11",
    "eslint": "^8.36.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "is-electron": "^2.2.2",
    "material-ui-popup-state": "^5.0.5",
    "mui-color-input": "^1.0.5",
    "prebuild-install": "^7.1.1",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.9.0",
    "react-transition-group": "^4.4.5",
    "sass": "^1.59.2",
    "simple-peer": "^9.11.1",
    "socket.io-client": "^4.6.1",
    "typescript": "^4.9.5",
    "vite": "^3.2.5",
    "vite-electron-plugin": "^0.7.4"
